{
  "kind": "collectionType",
  "collectionName": "client_profiles",
  "info": {
    "singularName": "client-profile",
    "pluralName": "client-profiles",
    "displayName": "clientProfile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "role": {
      "type": "enumeration",
      "enum": [
        "user",
        "broker",
        "realtor",
        "real estate laywer"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "client_profile"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "birthday": {
      "type": "date"
    },
    "profile_pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phone": {
      "type": "string"
    },
    "professional_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::professional-profile.professional-profile",
      "mappedBy": "client_profile"
    },
    "agency": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::agency.agency",
      "mappedBy": "client_profile"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "client_profile"
    },
    "testimonials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::testimonial.testimonial",
      "mappedBy": "client_profile"
    },
    "submitted_testimonials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::testimonial.testimonial",
      "mappedBy": "testimonial_user"
    },
    "listings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::listing.listing",
      "mappedBy": "client_profile"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "client_profile"
    },
    "portfolios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::portfolio.portfolio",
      "mappedBy": "client_profile"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "client_profile"
    },
    "certifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::certification.certification",
      "mappedBy": "client_profile"
    },
    "preference": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::preference.preference",
      "mappedBy": "client_profile"
    },
    "favourites": {
      "type": "json"
    },
    "views": {
      "type": "json"
    },
    "stripeCustomerId": {
      "type": "string"
    },
    "stripeSubscriptionId": {
      "type": "string"
    },
    "stripePriceId": {
      "type": "string"
    },
    "stripeCurrentPeriodEnd": {
      "type": "date"
    },
    "planId": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "client_profile"
    }
  }
}
